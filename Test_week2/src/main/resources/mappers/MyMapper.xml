<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace 命名空间 早期版本是可选的 现在是必须的 可以是随意的名称 但是最好是全限定名称 -->
<!-- 命名空间使用的是对应的接口的全限定名称 -->
<mapper namespace="com.bw.mapper.MyMapper">
	<insert parameterType="newsLink" id="add">
		insert into t_news_link(weight,created)values (#{weight},#{created})
	</insert>
	<select id="list" resultType="newsLink">
		select * from t_news_link
	</select>
	
</mapper>